<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
<link rel="stylesheet" href="css/account.css"/> 
<link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.4.0/css/font-awesome.min.css" rel="stylesheet" type="text/css"> <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
<script src="js/jquery-1.11.1.min.js"></script>
<div class="container"> 
    <div class="row"> 
     <h1 class="text-center">List Account</h1> 
     <div class="col-md-10 col-md-offset-1"> 
      <div class="panel panel-default panel-table" style="width: 1120px;"> 
       <div class="panel-heading"> 
        <div class="row"> 
         <div class="col col-xs-6"> 
          <h3 class="panel-title">Account</h3> 
         </div> 
         <div class="col col-xs-6 text-right"> 
         </div>   
        </div> 
       </div> 
       <div class="panel-body"> 
        <table class="table table-striped table-bordered table-list"> 
         <thead> 
          <tr> 
           <th><em class="fa fa-cog"></em>
           </th> 
           <th class="hidden-xs">ID</th> 
           <th>UserName</th> 
           <th>Email</th>
           <th>Enable</th>
           <th>Enable From</th>
           <th>Avatar</th>
          </tr> 
         </thead> 
         <tbody>
          <%
        if(userData.length!=0){
        var i=1;
        userData.forEach(function(data){
        %>
        <tr>
          <tr>
            <td align="center">
              <form action="managers" method="POST">
                <a class="btn btn-default" href="/manager?id=<%=data.id %>" type="submit" name="aaa"><em class="fa fa-pencil" ></em></a>
              </form>
            </td>
            <td class="hidden-xs"><%=data.id %></td>
            <td><%=data.name %></td>
            <td><%=data.email %></td>
            <td><input type="text" value="<%= data.avatar %>" name="avatar"/></td>
            <td><input type="text" value="<%= data.isEnable %>" name="enable"/></td>
            <td><input type="date" value="${data.enableFrom}" name="dateEnable"/><%=data.enableFrom %></td>
           </tr>
        </tr>
        <%  i++; }) %>
        <% } else{ %>
            <tr>
                <td colspan="7">No Data Found</td>
            </tr>
        <% } %>

        </tbody></table>
       </div>
       <div class="panel-footer">
        <div class="row"> 
         <div class="col col-xs-4">Trang 1 của 5 </div> 
         <div class="col col-xs-8"> 
          <ul class="pagination hidden-xs pull-right"> 
           <li><a href="http://hocwebgiare.com/#">1</a>
           </li> 
           <li><a href="http://hocwebgiare.com/#">2</a>
           </li> 
           <li><a href="http://hocwebgiare.com/#">3</a>
           </li> 
           <li><a href="http://hocwebgiare.com/#">4</a>
           </li> 
           <li><a href="http://hocwebgiare.com/#">5</a>
           </li> 
          </ul> 
          <ul class="pagination visible-xs pull-right"> 
           <li><a href="http://hocwebgiare.com/#">«</a>
           </li> 
           <li><a href="http://hocwebgiare.com/#">»</a>
           </li> 
          </ul> 
         </div> 
        </div> 
       </div> 
      </div> 
     </div> 
    </div>
    <div class="buttonLogOut" style="padding: 93px; font-size: large">
        <p>Log-out
            <a href="/logout">
              <span class="glyphicon glyphicon-log-out"></span>
            </a>
          </p>
     </div>
   </div>
